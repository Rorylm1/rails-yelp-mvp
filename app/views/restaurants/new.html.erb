<%= form_with model: @restaurant do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name%>
  <%= form.label :address %>
  <%= form.text_field :address%>
    <%= form.label :phone_number %>
  <%= form.text_field :number%>
  <%= form.label :category %>
   <%= form.select :category, collection: Restaurant::CATEGORY %>
  <%= form.submit %>
<% end %>
